{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/([a-zA-Z0-9]{6})",
      "dest": "/public/interstitial.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/signup",
      "dest": "/public/signup.html"
    },
    {
      "src": "/reset-password",
      "dest": "/public/reset-password.html"
    },
    {
      "src": "/(.*\\.(html|css|js))",
      "dest": "/public/$1"
    },
    {
      "src": "/(index.html)?",
      "dest": "/public/index.html"
    }
  ]
}
